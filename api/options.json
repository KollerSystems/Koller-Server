{
  "authorization": {
    "tokenLength": 32,
    "allowedCharacters": "0123456789abcdefghijklmnpqrstuvwxyz-._~/",
    "expiry": {
      "accessToken": 86400,
      "refreshToken": 8640000
    }
  },
  "databaseLoginData": {
    "user": "root",
    "password": ""
  },
  "api": {
    "port": 80,
    "exitTimeout": 2000,
    "batchRequests": {
      "defaultLimit": 25,
      "maxLimit": 50,
      "allowedRoles": ["student", "teacher"]
    }
  },
  "logging": {
    "logAsISOString": false,
    "logIP": false,
    "logUnsuccessful": true,
    "logNotFound": false,
    "logConsole": true,
    "logFile": "",
    "overwriteLog": false
  },
  "errorChecking": {
    "extendPermissions": true,
    "database": true,
    "options": true
  }
}